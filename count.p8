pico-8 cartridge // http://www.pico-8.com
version 15
__lua__

function _init()
	tx=40 ty=10
	gr=0.01 vy=0 vx=0
	thrust=0.02
	thrustb=5
	explodeb=4
	shipspr=0
	rot=0
	rotrate=5
	sprrot=0
	dx=0
	dy=1
	ground=0
	x=32 y=ground
	time=0
	slowtime=0
	camx=0 camy=0
	deadship=false
	engine=true
	explodetimer=0
	maxexplode=25
	moved=false
	mincamx=64 maxcamx=128*8-64
	mincamy=-40
	landed=true

	message = {}
	message.msg = ""
	message.col = 0
	message.maxtime = 0
end

function _draw()
	cls()
	camera()
	drawstatus()
	movecam()
	drawstars()
	drawmap()
	drawship()
end

function _update()
	resetgame()
	time=time+1
	if ((time % 5) == 0) then slowtime = slowtime + 1 end
	if (time > 32000) then time = 0 slowtime = 0 end
	dorot()
	if (not deadship) then 
		if (btn(explodeb)) then
			explode()
		else
			moveship()
		end
	elseif (explodetimer > 0) then
		moveship()
	end
	collide()
end

function moveship()
	vy=vy+gr
	if (canthrust() and btn(thrustb)) then
		moved=true
		landed=false
		vy=vy-thrust*dy
		vx=vx+thrust*dx
		shipspr=32+2*sprrot
	else
		shipspr=2*sprrot
	end
	if (landed or deadship) then
		vy = 0 vx = 0
	else
		y=y+vy
		x=x+vx
	end
	if (not engine and y > -500) then
		engine=true
		send("engine restarted", 11)
	elseif (engine and y < -500) then
		engine=false
		send("engine failure!", 8)
	end
end

function drawship()
	if (not deadship) then
		spr(shipspr,x,y,2,2)
	elseif (explodetimer > 0) then
		explodetimer -= 1
		frame=flr(5*explodetimer/maxexplode)
		if (frame >= 3) then
			spr(shipspr,x,y,2,2)
		end
		spr(136-2*frame,x,y,2,2)
	end
end

function send(msg,col)
	message.msg = msg
	message.col = col
	message.maxtime = 100
end

function drawstatus()
	if (message.maxtime > 0) then
		message.maxtime -= 1
		printat (1, message.msg, message.col)
	end
	if (not moved) then
		printat (2, "x to fire engine", 12)
		printat (3, "z to reset", 12)
	end
	-- printat(0, "vx:"..vx.." vy:"..vy, 9)
	-- printat(15, "camx:"..camx.." x:"..x.." y:"..y,10)
end

function collide()
	spr1 = fget(mget(flr((x+1)/8),flr((y-8)/8)+30))
	spr2 = fget(mget(flr((x+14)/8),flr((y-8)/8)+30))
	spr3 = fget(mget(flr((x+1)/8),flr((y+7)/8)+30))
	spr4 = fget(mget(flr((x+14)/8),flr((y+7)/8)+30))
	collision = band(bor(bor(spr1,spr2),bor(spr3,spr4)),1)
	if (collision == 1) then
		printat(0, "collision!!!!!",10) 
		if (not deadship) then explode() end
	end
	landed = (band(band(spr3,spr4),2) == 2) and abs(vx) < 0.1 and vy >= 0 and vy < 0.5 and (y%8 == 0 or y%8 > 5)
	if (landed) then
		sprrot = 0
		vx = 0
		vy = 0
		dx=0 dy=1
	end
end

function printat(row,msg,col)
	print (msg, 64-2*#msg, row*8, col)
end

function canthrust()
	return engine
end

function movecam()
	camx=max(mincamx, min(x,maxcamx))
	camy=min(y,mincamy)
	camera(camx-64,camy-64)
end

function explode()
	deadship = true
	explodetimer=maxexplode
	send("warp core breach!", 10)
	vx=0
end

function resetgame()
	if (deadship and explodetimer==0 and message.maxtime==0) then
		_init()
	end
end

function drawstars()
	srand(1234)
	for i=1,100 do
		loc = (rnd(8192)-x/20-camx-128*flr(camy))%8192
		col = rnd(2)+5
		if ((slowtime+i)%100 != 0) then
			poke(0x6000+loc, col)
		end
	end
end

function diag()
	print ("x:",tx,ty,10)
	print (x,tx+8,ty)
	print ("y:",tx,ty+8,10)
	print (y,tx+8,ty+8)
	print ("r:",tx,ty+16,10)
	print (dx,tx+8,ty+16)
	for i=0,7 do
		if (btn(i)) then
			print (i,tx,ty+16+8*i)
		end
	end
end

function dorot()
	if (deadship) then return end
	if (btn(0)) then rot=rot-rotrate end
	if (btn(1)) then rot=rot+rotrate end
	if rot < 0 then rot=rot+360 end
	if rot > 360 then rot=rot-360 end
	if rot >= 337.5 or rot < 22.5 then sprrot=0
	elseif rot < 67.5 then sprrot=1
	elseif rot < 112.5 then sprrot=2
	elseif rot < 157.5 then sprrot=3
	elseif rot < 202.5 then sprrot=4
	elseif rot < 247.5 then sprrot=5
	elseif rot < 292.5 then sprrot=6
	else sprrot=7 end
	dx = -sin(sprrot/8)
	dy = cos(sprrot/8)
end

function drawmap()
	map(0,0,0,-29*8,128,128)
end

